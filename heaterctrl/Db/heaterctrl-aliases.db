# Aliases for the heaterctl for anticryostat
#
# temp sensors, expecting P=CstatV-AC:, DEV1=DAQ6510
# heaters, expecting PSU1=CstatV-AC1:, PSU2=CstatV-AC2:, DEV2=HMP4040
#
alias("${P}${DEV1}:Chan01:RdTemp", "CstatV-AC1:TT1A:sRdV")
alias("${P}${DEV1}:Chan02:RdTemp", "CstatV-AC1:TT1B:sRdV")
alias("${P}${DEV1}:Chan03:RdTemp", "CstatV-AC1:TT2A:sRdV")
alias("${P}${DEV1}:Chan04:RdTemp", "CstatV-AC1:TT2B:sRdV")
alias("${P}${DEV1}:Chan05:RdTemp", "CstatV-AC1:TT3A:sRdV")
alias("${P}${DEV1}:Chan06:RdTemp", "CstatV-AC1:TT4A:sRdV")
alias("${P}${DEV1}:Chan07:RdTemp", "CstatV-AC1:TT4B:sRdV")
alias("${P}${DEV1}:Chan08:RdTemp", "CstatV-AC1:TT5A:sRdV")

alias("${P}${DEV1}:Chan11:RdTemp", "CstatV-AC2:TT1A:sRdV")
alias("${P}${DEV1}:Chan12:RdTemp", "CstatV-AC2:TT1B:sRdV")
alias("${P}${DEV1}:Chan13:RdTemp", "CstatV-AC2:TT2A:sRdV")
alias("${P}${DEV1}:Chan14:RdTemp", "CstatV-AC2:TT2B:sRdV")
alias("${P}${DEV1}:Chan15:RdTemp", "CstatV-AC2:TT3A:sRdV")
alias("${P}${DEV1}:Chan16:RdTemp", "CstatV-AC2:TT4A:sRdV")
alias("${P}${DEV1}:Chan17:RdTemp", "CstatV-AC2:TT4B:sRdV")
alias("${P}${DEV1}:Chan18:RdTemp", "CstatV-AC2:TT5A:sRdV")
# 
# heaters
#
alias("${PSU1}${DEV2}:Chan1:RdMeasVolt", "CstatV-AC1:EH1AB-I:sRdV")
alias("${PSU1}${DEV2}:Chan1:RdMeasCurr", "CstatV-AC1:EH1AB-V:sRdV")
alias("${PSU1}${DEV2}:Chan2:RdMeasVolt", "CstatV-AC1:EH2AB-I:sRdV")
alias("${PSU1}${DEV2}:Chan2:RdMeasCurr", "CstatV-AC1:EH2AB-V:sRdV")
alias("${PSU1}${DEV2}:Chan3:RdMeasVolt", "CstatV-AC1:EH3AB-I:sRdV")
alias("${PSU1}${DEV2}:Chan3:RdMeasCurr", "CstatV-AC1:EH3AB-V:sRdV")
alias("${PSU1}${DEV2}:Chan4:RdMeasVolt", "CstatV-AC1:EH4AB-I:sRdV")
alias("${PSU1}${DEV2}:Chan4:RdMeasCurr", "CstatV-AC1:EH4AB-V:sRdV")

alias("${PSU2}${DEV2}:Chan1:RdMeasVolt", "CstatV-AC2:EH1AB-I:sRdV")
alias("${PSU2}${DEV2}:Chan1:RdMeasCurr", "CstatV-AC2:EH1AB-V:sRdV")
alias("${PSU2}${DEV2}:Chan2:RdMeasVolt", "CstatV-AC2:EH2AB-I:sRdV")
alias("${PSU2}${DEV2}:Chan2:RdMeasCurr", "CstatV-AC2:EH2AB-V:sRdV")
alias("${PSU2}${DEV2}:Chan3:RdMeasVolt", "CstatV-AC2:EH3AB-I:sRdV")
alias("${PSU2}${DEV2}:Chan3:RdMeasCurr", "CstatV-AC2:EH3AB-V:sRdV")
alias("${PSU2}${DEV2}:Chan4:RdMeasVolt", "CstatV-AC2:EH4AB-I:sRdV")
